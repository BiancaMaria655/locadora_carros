<p>Olá, {{customer.name}}.</p>



<div class="title">
    <h1>Todas as Locações Realizadas</h1>
</div>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">Nome Locatario</th>
      <th scope="col">Veiculo</th>
      <th scope="col">Data de Inicio da Locação</th>
      <th scope="col">Data Final da Locação</th>
      <th scope="col">Valor Total da Locação</th>
      <th scope="col">Status Locação</th>
    </tr>
  </thead>
  <tbody>
    {{#each loc}}
    <tr>      
      <td>{{this.nome}}</td>
      <td>{{this.veiculo}}</td>
      <td>{{this.dataIn}}</td>
      <td>{{this.dataFim}}</td>
      <td>{{this.valor}}</td>
      <td>{{this.status}}</td>
      <td>
        <div class="col-md-4 actions">
        <a href="/locacao/editar/{{this.id}}">Editar</a>
        </div>
      </td>
    </tr>
    {{/each}}
  </tbody>
</table>