<div class="users container p-2">
  {{#if success}}
    <div class="aler alert-success mb-3">
      {{success}}
    </div>

  {{/if}}
  
    {{>formAdmin}}

  <div class="border p-2">
    {{#if users}}
    <div class=" row justify-content-start table-bordered">
      
      <thead class="table-bordered">

        <th scope="col"><div class="col"><label for="">Nome</label></div></th>
        <th scope="col"><div class="col-2"><label for="">Email</label></div></th>
        <div class="col"><label for="">Senha</label></div>
        <div class="col"><label for="">CPF</label></div>
        <div class="col"><label for="">Tipo</label></div>
        <div class="col"><label for="">Ações</label></div>
      </thead>


    </div>  
  {{/if}}
  
    {{#each users}}
      <div id="adminList">
        {{>admin}}

      </div>
    
    {{/each}}
  </div>
 
</div>

<div class="cars container p-2">
  <div class="border p-2">
    {{#if cars}}
    <div class=" row justify-content-start table-bordered">
      
      <thead class="table-bordered">
        <tr>
          <th scope="col">#</th>
          <th scope="col">Nome</th>
          <th scope="col">Modelo</th>
          <th scope="col">Ano</th>
          <th scope="col">Fabricante</th>
          <th scope="col">Valor</th>
          <th scope="col">Cor</th>
          <th scope="col">Disponivel</th>
          <th scope="col">Adicionais</th>

        </tr>
        
      </thead>
    </div>  
  {{/if}}
    <tbody>

      {{#each cars}}
      
          {{>cars}}
      
      {{/each}}
    </tbody>
  </div>