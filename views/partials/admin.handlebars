<tr id="{{this.id}}">
  <th scope="row" id="id{{this.id}}">{{this.id}}</th>
  <td id="name{{this.id}}">{{this.name}}</td>
  <td id="email{{this.id}}">{{this.email}}</td>
  <td id="pass{{this.id}}">{{this.password}}</td>
  <td id="cpf{{this.id}}">{{this.cpf}}</td>
  <td id="type{{this.id}}">{{this.user_type}}</td>
  <td>
    <div class="buttons d-flex">
      <div class="pe-1">
        <a onclick="editFields(btnE{{this.id}})" class="btn btn-warning" name="btnE" id="btnE{{this.id}}">Editar</a>
      </div>
      <form action="/admin/excluir" method="POST" class='' >
        <input type="text" name="id" value="{{this.id}}", hidden>
        <input type="submit" value="Excluir" class="btn btn-danger" >
      </form>
  </td>
</tr>

